<html lang="en">
 <head>
  <meta charset="utf-8"/>
  <meta content="width=device-width, initial-scale=1" name="viewport"/>
  <title>
   Practice Fusion Schedule
  </title>
  <link crossorigin="anonymous" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" rel="stylesheet"/>
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet"/>
  <style>
    body {
      margin: 0;
      font-family: Arial, sans-serif;
      background-color: #fff;
    }
    .topbar {
      background-color: #333333;
      color: #ccc;
      font-size: 13px;
      height: 30px;
      line-height: 30px;
      padding: 0 10px;
      display: flex;
      justify-content: flex-end;
      gap: 15px;
      user-select: none;
    }
    .topbar .help {
      margin-right: auto;
      cursor: pointer;
      color: #ccc;
    }
    .topbar .user-info {
      display: flex;
      align-items: center;
      gap: 10px;
    }
    .topbar .user-info > * {
      cursor: default;
    }
    .topbar .icon {
      color: #ccc;
      cursor: pointer;
    }
    .sidebar {
      background-color: #1a1a1a;
      width: 60px;
      min-width: 60px;
      height: calc(100vh - 30px);
      display: flex;
      flex-direction: column;
      align-items: center;
      padding-top: 10px;
      gap: 15px;
      user-select: none;
    }
    .sidebar .logo {
      width: 40px;
      height: 40px;
      margin-bottom: 5px;
    }
    .sidebar .logo img {
      width: 40px;
      height: 40px;
      object-fit: contain;
    }
    .sidebar .logo-text {
      font-size: 10px;
      color: #3bb4f2;
      font-weight: 600;
      letter-spacing: 0.5px;
      margin-top: 2px;
      text-align: center;
      line-height: 1;
      margin-top: -15px;
    }
    .sidebar button {
      background: none;
      border: none;
      color: #999999;
      font-size: 20px;
      width: 40px;
      height: 40px;
      display: flex;
      justify-content: center;
      align-items: center;
      cursor: pointer;
    }
    .sidebar button.active, .sidebar button:hover {
      color: #3bb4f2;
    }
    .main-container {
      height: calc(100vh - 30px);
      width: calc(100vw - 60px)
      display: flex;
      flex-direction: column;
    }
    .header-blue {
      background-color: #2196f3;
      color: white;
      padding: 10px 15px;
      font-size: 20px;
      font-weight: 400;
      user-select: none;
      display: flex;
      align-items: center;
    }
    .header-blue .appointments-count {
      font-weight: 600;
      margin-left: 5px;
    }
    .nav-tabs-custom {
      background-color: #2196f3;
      padding-left: 15px;
      padding-top: 5px;
    }
    .nav-tabs-custom .nav-link {
      color: white;
      font-size: 14px;
      font-weight: 600;
      padding: 5px 12px;
      border: none;
      border-radius: 0;
    }
    .nav-tabs-custom .nav-link.active {
      background-color: #0b7dda;
      color: white;
    }
    .filter-bar {
      background-color: white;
      border-top: 1px solid #ccc;
      border-bottom: 1px solid #ccc;
      padding: 5px 15px;
      display: flex;
      align-items: center;
      gap: 10px;
    }
    .filter-bar .btn {
      font-size: 13px;
      padding: 3px 10px;
    }
    .filter-bar select {
      font-size: 13px;
      height: 28px;
      padding: 2px 6px;
    }
    .filter-bar .btn-print {
      margin-left: auto;
      max-width: 180px;
      margin-right: 25px;
    }
    .content-area {
      display: flex;
      height: 100%;
      overflow: hidden;
    }
    .sidebar-left {
      width: 220px;
      border-right: 1px solid #ccc;
      background-color: #f8f8f8;
      overflow-y: auto;
      padding: 10px 10px 0 10px;
      font-size: 13px;
      user-select: none;
    }
    .sidebar-left .section-title {
      font-weight: 600;
      font-size: 12px;
      margin-bottom: 5px;
      display: flex;
      align-items: center;
      gap: 5px;
    }
    .sidebar-left .section-title i {
      font-size: 14px;
    }
    .sidebar-left .checkbox-group {
      margin-bottom: 15px;
    }
    .sidebar-left label {
      display: flex;
      align-items: center;
      gap: 5px;
      cursor: pointer;
    }
    .sidebar-left label input[type="checkbox"] {
      cursor: pointer;
    }
    .sidebar-left label .color-box {
      width: 12px;
      height: 12px;
      display: inline-block;
      border-radius: 2px;
    }
    .color-purple { background-color: #7f3fbf; }
    .color-orange { background-color: #d18a00; }
    .color-green { background-color: #3dbb00; }
    .sidebar-left small.text-primary {
      font-size: 12px;
      color: #2196f3;
      cursor: pointer;
      user-select: none;
    }
    .expandable {
      font-weight: 600;
      font-size: 12px;
      cursor: pointer;
      user-select: none;
      margin-bottom: 10px;
      display: flex;
      align-items: center;
      gap: 5px;
    }
    .expandable i {
      font-size: 10px;
    }
    .main-content {
      flex-grow: 1;
      overflow-x: auto;
      overflow-y: hidden;
      background-color: white;
    }
    .date-bar {
      display: flex;
      align-items: center;
      padding: 8px 15px;
      font-size: 14px;
      font-weight: 600;
      border-bottom: 1px solid #ccc;
      user-select: none;
      gap: 10px;
      flex-wrap: wrap;
    }
    .date-bar .view-select {
      margin-left: auto;
      margin-right: 10px;
      font-size: 13px;
      height: 28px;
      padding: 2px 6px;
      min-width: 130px;
    }
    .date-bar .btn-group {
      margin-right: 10px;
    }
    .date-bar .btn {
      font-size: 13px;
      padding: 3px 8px;
    }
    .date-bar .btn.today {
      background-color: #2196f3;
      color: white;
      border: none;
    }
    .date-bar .btn.today:hover {
      background-color: #0b7dda;
    }
    .date-bar .search-group {
      display: flex;
      align-items: center;
      gap: 5px;
      margin-right: 10px;
    }
    .date-bar input[type="search"] {
      font-size: 13px;
      height: 28px;
      padding: 2px 6px;
      width: 120px;
    }
    .date-bar select.slot-select {
      font-size: 13px;
      height: 28px;
      padding: 2px 6px;
      max-width: 130px;
    }
    table.schedule-table {
      width: 100%;
      border-collapse: collapse;
      font-size: 12px;
    }
    table.schedule-table thead tr {
      background-color: #d3d3d3;
      color: #333;
      user-select: none;
    }
    table.schedule-table thead th {
      padding: 6px 8px;
      border: 1px solid #bbb;
      text-align: left;
      white-space: nowrap;
    }
    table.schedule-table thead th.time-header {
      cursor: pointer;
    }
    table.schedule-table thead th.time-header i {
      margin-left: 4px;
    }
    table.schedule-table tbody tr {
      height: 40px;
    }
    table.schedule-table tbody tr td {
      border: 1px solid #eee;
      padding: 6px 8px;
      color: #666;
    }
    .no-appointments {
      padding: 40px 0;
      text-align: center;
      color: #999999;
      font-weight: 600;
      font-size: 14px;
      user-select: none;
    }
    .advertisement {
      width: 160px;
      border-left: 1px solid #ccc;
      background-color: white;
      font-family: Arial, sans-serif;
      font-size: 13px;
      color: #333;
      padding: 10px 10px 10px 10px;
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      user-select: none;
    }
    .advertisement .ad-image {
      width: 100%;
      height: 120px;
      object-fit: contain;
      margin-bottom: 10px;
    }
    .advertisement .ad-text {
      flex-grow: 1;
    }
    .advertisement .ad-text h5 {
      font-weight: 600;
      font-size: 16px;
      margin-bottom: 10px;
      line-height: 1.2;
    }
    .advertisement .ad-text p {
      font-size: 12px;
      margin-bottom: 10px;
      line-height: 1.3;
    }
    .advertisement .ad-button {
      background-color: #f37021;
      color: white;
      border: none;
      font-size: 13px;
      font-weight: 600;
      padding: 6px 10px;
      cursor: pointer;
      border-radius: 3px;
      text-align: center;
    }
    .advertisement .ad-button:hover {
      background-color: #d65c00;
    }
    .bottom-right-icons {
      position: fixed;
      bottom: 10px;
      right: 10px;
      display: flex;
      flex-direction: column;
      gap: 10px;
      z-index: 1000;
    }
    .bottom-right-icons button {
      background-color: #f37021;
      border: none;
      border-radius: 50%;
      width: 44px;
      height: 44px;
      color: white;
      font-size: 20px;
      cursor: pointer;
      display: flex;
      justify-content: center;
      align-items: center;
    }
    .bottom-right-icons button:nth-child(2) {
      background-color: #6f42c1;
    }
    /* Scrollbar for left sidebar */
    .sidebar-left::-webkit-scrollbar {
      width: 6px;
    }
    .sidebar-left::-webkit-scrollbar-thumb {
      background-color: #ccc;
      border-radius: 3px;
    }
    /* Scrollbar for main content horizontal */
    .main-content::-webkit-scrollbar {
      height: 8px;
    }
    .main-content::-webkit-scrollbar-thumb {
      background-color: #ccc;
      border-radius: 4px;
    }
    /* Fix topbar and sidebar to top */
    .topbar {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      z-index: 1050;
    }
    .sidebar {
      position: fixed;
      top: 30px;
      left: 0;
      bottom: 0;
      z-index: 1040;
    }
    .main-container {
      width: calc(100vw - 60px);
      display: flex;
      flex-direction: column;
    }
    .content-area {
      height: 100%;
    }
    /* Remove white space between sidebar and main content */
    .content-area {
      gap: 0;
    }
    @media (max-width: 768px) {
      .sidebar {
        width: 50px;
      }
      .sidebar .logo-text {
        display: none;
      }
      .sidebar-left {
        display: none;
      }
      .advertisement {
        display: none;
      }
    }
    .container, .container-lg, .container-md, .container-sm, .container-xl {
        max-width: 1269px;
        margin-top: 31px;
    }
      .header-bar {
        background-color: #3a8ee6;
        color: white;
        font-weight: 500;
        font-size: 16px;
        padding: 6px 12px;
        border-bottom: 3px solid #2a6ed1;
      }
      .query-title {
        font-weight: 600;
        font-size: 13px;
      }
      .query-title .fa-pencil-alt {
        font-size: 11px;
        margin-left: 4px;
        cursor: pointer;
        color: #3a8ee6;
      }
      .alert-saved {
        background-color: #d9f0d9;
        border: 1px solid #a6d8a6;
        color: #3a8e3a;
        font-size: 13px;
        padding: 6px 12px;
        border-radius: 4px;
        display: inline-flex;
        align-items: center;
        gap: 6px;
      }
      .alert-saved i {
        font-size: 14px;
      }
      .btn-group-sm > .btn, .btn-sm {
        font-size: 13px;
        padding: 2px 8px;
      }
      .btn-save {
        background-color: #0d6efd;
        border-color: #0d6efd;
        font-size: 13px;
        padding: 2px 8px;
      }
      .btn-save:hover, .btn-save:focus {
        background-color: #0b5ed7;
        border-color: #0a58ca;
      }
      .btn-run {
        background-color: #f76707;
        border-color: #f76707;
        font-size: 13px;
        padding: 2px 8px;
      }
      .btn-run:hover, .btn-run:focus {
        background-color: #d65a06;
        border-color: #b04a05;
      }
      .form-select-sm {
        font-size: 13px;
        height: 28px;
        padding: 2px 6px;
      }
      .form-control-sm {
        font-size: 13px;
        height: 28px;
        padding: 2px 6px;
      }
      .input-group-text {
        font-size: 13px;
        padding: 2px 6px;
        background: white;
        border-left: none;
      }
      .search-input {
        font-size: 13px;
        height: 28px;
        padding: 2px 6px;
        border-right: none;
      }
      .table thead th {
        font-weight: 600;
        font-size: 12px;
        color: #6c757d;
        vertical-align: middle;
        border-bottom: 1px solid #dee2e6;
      }
      .table tbody td {
        font-size: 13px;
        vertical-align: middle;
        border-top: 1px solid #dee2e6;
      }
      .table tbody tr:hover {
        background-color: #f8f9fa;
      }
      .checkbox-cell {
        width: 24px;
        text-align: center;
      }
      .expand-cell {
        width: 40px;
        font-size: 12px;
        color: #0d6efd;
        cursor: pointer;
      }
      .status-active {
        font-size: 13px;
        color: #212529;
      }
      .preferred-contact {
        font-size: 13px;
        color: #212529;
      }
      .link-more {
        font-size: 13px;
        color: #0d6efd;
        cursor: pointer;
        text-decoration: none;
      }
      .link-more:hover, .link-more:focus {
        text-decoration: underline;
      }
      .and-text {
        font-size: 13px;
        font-weight: 600;
        margin-left: 8px;
        margin-right: 8px;
        user-select: none;
      }
      .filters-row {
        gap: 8px;
      }
      /* Adjust dropdown toggle icon to be smaller and aligned */
      .btn-group-sm > .btn.dropdown-toggle::after {
        margin-left: 0.25rem;
        vertical-align: 0.15em;
        border-top: 0.3em solid;
        border-right: 0.3em solid transparent;
        border-left: 0.3em solid transparent;
      }
      @media (max-width: 575.98px) {
        .table-responsive {
          font-size: 12px;
        }
        .btn, .form-select-sm, .form-control-sm {
          font-size: 12px;
        }
      }
    </style>
 </head>
