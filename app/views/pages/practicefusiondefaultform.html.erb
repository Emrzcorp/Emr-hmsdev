<body>
  <!-- 🔐 Topbar -->
  <div class="topbar">
    <div class="help">Help <i class="fas fa-caret-down"></i></div>
    <div class="user-info">
      <div>Susan Martin</div>
      <div>|</div>
      <div>HeartWell Health <i class="fas fa-caret-down"></i></div>
      <div>|</div>
      <div><i class="fas fa-lock icon"></i></div>
      <div><i class="fas fa-cog icon"></i></div>
      <%= button_to destroy_user_session_path, method: :delete, class: "border-0 bg-transparent p-0 d-flex align-items-center text-danger ms-2" do %>
        <i class="fas fa-sign-out-alt me-2"></i>
        <span>Log out</span>
      <% end %>
    </div>
  </div>

  <!-- Sidebar -->
  <nav class="sidebar d-flex flex-column align-items-center">
    <div class="logo">
      <%= image_tag("unnamed.png", alt: "practice fusion image") %>
    </div>
    <div class="logo-text">practice fusion</div>

    <%# Sidebar buttons %>
    <% [["Home", "home"], ["Schedule", "calendar-alt"], ["Tasks", "tasks"], ["Charts", "file-medical-alt"], ["Messages", "envelope"], ["Reports", "chart-bar"]].each do |label, icon| %>
      <button aria-label="<%= label %>" title="<%= label %>">
        <i class="fas fa-<%= icon %>"></i>
      </button>
      <div class="logo-text"><%= label %></div>
    <% end %>
  </nav>

  <!-- Header section -->
  <div class="main-container ms-5">
    <div class="topbar mt-4">
      Settings <span class="separator">›</span> <a href="#">Patient intake forms</a>
      <a href="/ehrsetting" class="close-x">×</a>
    </div>
    <!-- Intake Forms Table -->
      <nav class="navbar navbar-expand navbar-custom px-3">
        <a class="navbar-brand fs-4" href="#">Patient intake form library</a>
        <span class="navbar-text">Manage patient intake forms</span>
      </nav>
      <div class="px-3 pt-2 border-bottom-1">
        <ul class="nav nav-tabs" role="tablist" style="max-width: 400px;">
          <li class="nav-item" role="presentation">
            <button class="nav-link active" id="intake-forms-tab" data-bs-toggle="tab" data-bs-target="#intake-forms" type="button" role="tab" aria-controls="intake-forms" aria-selected="true">Intake forms</button>
          </li>
          <li class="nav-item" role="presentation">
            <button class="nav-link" id="new-form-tab" type="button" onclick="window.location.href='intake.html'">
              Practice Fusion Default Form <i class="fas fa-times ms-1" style="font-size: 0.8rem;"></i>
            </button>
          </li>
        </ul><br>
      </div>  
      <div class="px-3 py-2 border-bottom-1 d-flex align-items-center" style="gap: 0.5rem;">
        <div class="form-check mb-0">
          <input class="form-check-input" type="checkbox" value="" id="defaultAppointments" />
          <label class="form-check-label" for="defaultAppointments">Set as default for Practice Fusion</label>
        </div>
        <div class="ms-auto d-flex gap-2 button-group-actions">
          
        </div>
        <button type="button" class="btn btn-outline-secondary btn-sm shadow-sm"
          onclick="window.location.href='<%= url_for('/reviewquestion') %>'">
          <i class="fas fa-eye me-1"></i> Preview
        </button>
        <button type="button" id="openModalBtn" class="btn btn-sm btn-orange shadow-sm">
            <i class="fas fa-copy me-1"></i> Make a copy
        </button>
        <!-- Custom Popup -->
        <div id="customModal" class="custom-modal">
          <div class="custom-modal-content">
            <span class="close-btn" id="closeModal">&times;</span>
